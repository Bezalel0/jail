local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe(data) m=string.sub(data, 0, 5555) data=data:gsub(m,'')

data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 


-- Combined ESP (Police and Criminals) and Aimbot Functionality

local ESP = {}
ESP.Enabled = true
local Players = game:GetService(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe(''))
local RunService = game:GetService(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe(''))
local UserInputService = game:GetService(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe(''))
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera
local ESPNames = {}
local MAX_DISTANCE = 10000
local MAX_AIMBOT_DISTANCE = 200 -- max distance is 200 studs

-- Aimbot Functionality
getgenv().toggled = true -- true & false
getgenv().old = getgenv().old or require(game:GetService(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')).Module.RayCast).RayIgnoreNonCollideWithIgnoreList

-- Check if on mobile
local isMobile = UserInputService.TouchEnabled and not UserInputService.MouseEnabled

local function CreateESPName(player)
    local Name = Drawing.new(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe(''))
    Name.Text = player.Name
    Name.Size = isMobile and 14 or 18  -- Smaller size for mobile
    Name.Center = true
    Name.Outline = true
    Name.Visible = false
    return Name
end

local function UpdateESPName(player, espName, rootPart)
    local rootPosition, onScreen = Camera:WorldToViewportPoint(rootPart.Position)
    local distance = (rootPart.Position - Camera.CFrame.Position).Magnitude
    
    if onScreen and ESP.Enabled then
        espName.Position = Vector2.new(rootPosition.X, rootPosition.Y - 15)
        espName.Size = isMobile and math.clamp(20 - (distance / 100), 8, 20) or math.clamp(24 - (distance / 100), 10, 24)
        espName.Transparency = math.clamp((MAX_DISTANCE - distance) / MAX_DISTANCE, 0.3, 1)
        espName.Visible = true
        espName.Color = player.Team.Name == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('') and Color3.fromRGB(0, 0, 255) or Color3.fromRGB(255, 0, 0)  -- Blue for Police, Red for Criminals
    else
        espName.Visible = false
    end
end

local function CreateESPForPlayer(player)
    if player ~= LocalPlayer and player.Team and (player.Team.Name == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('') or player.Team.Name == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')) then
        if not ESPNames[player] then
            ESPNames[player] = CreateESPName(player)
        end
    end
end

local function RemoveESPForPlayer(player)
    if ESPNames[player] then
        ESPNames[player]:Remove()
        ESPNames[player] = nil
    end
end

local function HandleTeamChange(player)
    if player.Team and (player.Team.Name == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('') or player.Team.Name == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')) then
        CreateESPForPlayer(player)
    else
        RemoveESPForPlayer(player)
    end
end

-- Initialize ESP for existing players
for _, player in ipairs(Players:GetPlayers()) do
    player:GetPropertyChangedSignal(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')):Connect(function()
        HandleTeamChange(player)
    end)
    HandleTeamChange(player)
end

-- Handle new players joining
Players.PlayerAdded:Connect(function(player)
    player:GetPropertyChangedSignal(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')):Connect(function()
        HandleTeamChange(player)
    end)
    HandleTeamChange(player)
end)

-- Handle players leaving
Players.PlayerRemoving:Connect(RemoveESPForPlayer)

-- ESP Update Function
local function UpdateESP()
    for player, espName in pairs(ESPNames) do
        local character = player.Character
        local rootPart = character and character:FindFirstChild(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe(''))
        if rootPart and (player.Team.Name == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('') or player.Team.Name == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')) then
            UpdateESPName(player, espName, rootPart)
        else
            espName.Visible = false
        end
    end
end

-- Aimbot Function
local function AimbotFunction(...)
    local nearestDistance, nearestEnemy = MAX_AIMBOT_DISTANCE, nil
    for i, v in pairs(Players:GetPlayers()) do
        if v.Team ~= LocalPlayer.Team and v.Character and v.Character:FindFirstChild(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')) then
            local distance = (v.Character.HumanoidRootPart.Position - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
            if distance < nearestDistance then
                nearestDistance, nearestEnemy = distance, v
            end
        end
    end
    local arg = {getgenv().old(...)}
    if (tostring(getfenv(2).script) == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('') or tostring(getfenv(2).script) == vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')) and nearestEnemy then
        arg[1] = nearestEnemy.Character.HumanoidRootPart
        arg[2] = nearestEnemy.Character.HumanoidRootPart.Position
    end
    return unpack(arg)
end

-- Apply Aimbot
if getgenv().toggled then
    require(game:GetService(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')).Module.RayCast).RayIgnoreNonCollideWithIgnoreList = AimbotFunction
else
    require(game:GetService(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe('')).Module.RayCast).RayIgnoreNonCollideWithIgnoreList = getgenv().old
end

-- Run ESP Update
RunService:BindToRenderStep(vOFpTgrcjApmneJDIlmZtDnFsNVdRyfedaHukFe(''), Enum.RenderPriority.Camera.Value, UpdateESP)
