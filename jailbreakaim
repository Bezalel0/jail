local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC(data) m=string.sub(data, 0, 5555) data=data:gsub(m,'')

data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 




local ESP = {}
ESP.Enabled = true
local Players = game:GetService(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC(''))
local RunService = game:GetService(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC(''))
local UserInputService = game:GetService(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC(''))
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera
local ESPNames = {}
local MAX_DISTANCE = 10000
local MAX_AIMBOT_DISTANCE = 200 -- max distance is 200 studs


getgenv().toggled = true -- true & false
getgenv().old = getgenv().old or require(game:GetService(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')).Module.RayCast).RayIgnoreNonCollideWithIgnoreList

-- Check if on mobile
local isMobile = UserInputService.TouchEnabled and not UserInputService.MouseEnabled

local function CreateESPName(player)
    local Name = Drawing.new(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC(''))
    Name.Text = player.DisplayName
    Name.Size = isMobile and 14 or 18  -- Smaller size for mobile
    Name.Center = true
    Name.Outline = true
    Name.Visible = false
    return Name
end

local function UpdateESPName(player, espName, rootPart)
    local rootPosition, onScreen = Camera:WorldToViewportPoint(rootPart.Position)
    local distance = (rootPart.Position - Camera.CFrame.Position).Magnitude
    
    if onScreen and ESP.Enabled then
        espName.Position = Vector2.new(rootPosition.X, rootPosition.Y - 15)
        espName.Size = isMobile and math.clamp(20 - (distance / 100), 8, 20) or math.clamp(24 - (distance / 100), 10, 24)
        espName.Transparency = math.clamp((MAX_DISTANCE - distance) / MAX_DISTANCE, 0.3, 1)
        espName.Visible = true
        espName.Color = player.Team.Name == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('') and Color3.fromRGB(0, 0, 255) or Color3.fromRGB(255, 0, 0)  -- Blue for Police, Red for Criminals
    else
        espName.Visible = false
    end
end

local function CreateESPForPlayer(player)
    if player ~= LocalPlayer and player.Team and (player.Team.Name == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('') or player.Team.Name == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')) then
        if not ESPNames[player] then
            ESPNames[player] = CreateESPName(player)
        end
    end
end

local function RemoveESPForPlayer(player)
    if ESPNames[player] then
        ESPNames[player]:Remove()
        ESPNames[player] = nil
    end
end

local function HandleTeamChange(player)
    if player.Team and (player.Team.Name == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('') or player.Team.Name == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')) then
        CreateESPForPlayer(player)
    else
        RemoveESPForPlayer(player)
    end
    UpdateESP() -- Refresh the ESP
end


for _, player in ipairs(Players:GetPlayers()) do
    player:GetPropertyChangedSignal(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')):Connect(function()
        HandleTeamChange(player)
    end)
    HandleTeamChange(player)
end

-- Handle new players joining
Players.PlayerAdded:Connect(function(player)
    player:GetPropertyChangedSignal(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')):Connect(function()
        HandleTeamChange(player)
    end)
    HandleTeamChange(player)
end)


Players.PlayerRemoving:Connect(RemoveESPForPlayer)

-- ESP Update Function
local function UpdateESP()
    for player, espName in pairs(ESPNames) do
        local character = player.Character
        local rootPart = character and character:FindFirstChild(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC(''))
        if rootPart and (player.Team.Name == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('') or player.Team.Name == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')) then
            UpdateESPName(player, espName, rootPart)
        else
            espName.Visible = false
        end
    end
end

-- Refresh ESP
function ESP:Refresh()
    for player, espName in pairs(ESPNames) do
        HandleTeamChange(player)
    end
end


local function AimbotFunction(...)
    local nearestDistance, nearestEnemy = MAX_AIMBOT_DISTANCE, nil
    for i, v in pairs(Players:GetPlayers()) do
        if v.Team ~= LocalPlayer.Team and v.Character and v.Character:FindFirstChild(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')) then
            local distance = (v.Character.HumanoidRootPart.Position - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
            if distance < nearestDistance then
                nearestDistance, nearestEnemy = distance, v
            end
        end
    end
    local arg = {getgenv().old(...)}
    if (tostring(getfenv(2).script) == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('') or tostring(getfenv(2).script) == jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')) and nearestEnemy then
        arg[1] = nearestEnemy.Character.HumanoidRootPart
        arg[2] = nearestEnemy.Character.HumanoidRootPart.Position
    end
    return unpack(arg)
end


if getgenv().toggled then
    require(game:GetService(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')).Module.RayCast).RayIgnoreNonCollideWithIgnoreList = AimbotFunction
else
    require(game:GetService(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC('')).Module.RayCast).RayIgnoreNonCollideWithIgnoreList = getgenv().old
end


RunService:BindToRenderStep(jNEVUzqcvtADqOhlAnQkiAehxaZjVBUgiaJLqFinTzCplatNHmthtgMpwclFUIvitEbwKevNwxCsCmCGEwPacYpKhqWiJyJAkBiC(''), Enum.RenderPriority.Camera.Value, UpdateESP)
